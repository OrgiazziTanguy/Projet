<hibernate-mapping>
    <class name="model.Personne" 
        table="personne" catalog="jsfdb">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nom" type="string">
            <column name="nom" length="50" not-null="true" />
        </property>
        <property name="prenom" type="string">
            <column name="prenom" length="50" not-null="true" />
        </property>
        <property name="civil" type="string">
            <column name="civil" length="50" not-null="true" />
        </property>
        <property name="adresse" type="string">
            <column name="adresse" length="50" not-null="true" />
        </property>
        <property name="dateNaiss" type="timestamp">
            <column name="dateNaiss" length="50" not-null="true" />
        </property>
        <property name="tel" type="string">
            <column name="tel" length="50" not-null="true" />
        </property>
        <property name="mail" type="string">
            <column name="mail" length="50" not-null="true" />
        </property>
        <property name="notes" type="string">
            <column name="notes" length="50" not-null="true" />
        </property>
    </class>
</hibernate-mapping>